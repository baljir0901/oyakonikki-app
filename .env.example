# Supabase Configuration
VITE_SUPABASE_URL=your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# OAuth Providers
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_LINE_CLIENT_ID=your-line-client-id

# Domain Configuration (for local development)
VITE_MAIN_DOMAIN=localhost:8083
VITE_ADMIN_DOMAIN=admin-localhost:8083

# Payment Integration (if needed)
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key

# Feature Flags
VITE_ENABLE_LINE_LOGIN=true
VITE_ENABLE_GOOGLE_LOGIN=true
VITE_ENABLE_EMAIL_LOGIN=true

# API Configuration
VITE_API_URL=http://localhost:8083/api
VITE_API_TIMEOUT=30000

# Logging
VITE_LOG_LEVEL=debug

# Note: Create a .env.local file with your actual values
# Do not commit .env.local to version control
